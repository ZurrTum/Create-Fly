{
  "schemaVersion": 1,
  "id": "create",
  "version": "${version}",
  "name": "Create Fly",
  "description": "Creators-of-Create/Create port to fabric",
  "authors": [
    "ZurrTum"
  ],
  "contact": {
    "homepage": "https://github.com/ZurrTum/Create",
    "sources": "https://github.com/ZurrTum/Create"
  },
  "license": "CC0-1.0",
  "icon": "assets/pack.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.zurrtum.create.Create"
    ],
    "client": [
      "com.zurrtum.create.client.Create"
    ],
    "rei_client": [
      "com.zurrtum.create.client.compat.rei.ReiClientPlugin"
    ],
    "rei_common": [
      "com.zurrtum.create.compat.rei.ReiCommonPlugin"
    ],
    "jei_mod_plugin": [
      "com.zurrtum.create.client.compat.jei.JeiClientPlugin"
    ],
    "eiv": [
      "com.zurrtum.create.compat.eiv.EivCommonPlugin"
    ],
    "create_block": [
      {
        "adapter": "register",
        "value": "com.zurrtum.create.AllBlocks"
      }
    ],
    "create_fluid": [
      {
        "adapter": "register",
        "value": "com.zurrtum.create.AllFluids"
      }
    ]
  },
  "languageAdapters": {
    "register": "com.zurrtum.create.api.registry.CreateRegisterPlugin"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/world/Container": [
        "com/zurrtum/create/infrastructure/items/BaseInventory"
      ],
      "net/minecraft/world/WorldlyContainer": [
        "com/zurrtum/create/infrastructure/items/BaseSidedInventory"
      ]
    }
  },
  "accessWidener": "create.accesswidener",
  "mixins": [
    "create.mixins.json",
    {
      "config": "create.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.18.1",
    "minecraft": ">=1.21.11- <1.21.12-",
    "java": ">=21"
  }
}